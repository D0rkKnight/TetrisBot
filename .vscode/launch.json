    "version": "0.2.0",
    "configurations": [
        {
          "name": "Python C++ Debug",
          "type": "pythoncpp",
          "request": "launch",
          "pythonConfig": "default",
          "cppConfig": "default (win) Attach",
        },
        {
          "name": "(Windows) Attach",
          "type": "cppvsdbg",
          "request": "attach",
          "processId": "4820"
        },
        {
          "name": "Python: Current File",
          "type": "python",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal"
        }
    ]
}